7-


<?php
        #Declaração dos 5 Números

$num1 =9;
$num2 =8;
$num3=5;
$num4=10;
$num5=7;


      # Calculando do Maior numero 
$maiornum = max ($num1,$num2 , $num3 , $num4 , $num5)  ;

      # Maior número


echo"O maior numero é = ".$maiornum;

?>


