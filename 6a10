<?php
        #Declaração dos 10 Números

$num1 =10;
$num2 =8;
$num3=8;
$num4=10;
$num5=7;
$num6=10;
$num7=6;
$num8=5;
$num9=10;
$num10=5;

      # Soma dos 10 Números 
$somanum =  $num1 + $num2 + $num3 + $num4 + $num5 + $num6 + $num7 + $num8 + $num9 + $num10 ;

      # Media dos 10 Números

$resultado= $somanum / 10;

echo"A media dos 10 numeros = ".$resultado;

?>

